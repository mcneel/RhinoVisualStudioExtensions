<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="ProjectGroup" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
    <TemplateData>
        <Name>Rhino3D Plug-In (C++)</Name>
        <Description>Build plug-ins for Rhino3D using the C++ SDK</Description>
        <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>

        <SortOrder>20</SortOrder>

        <TemplateID>Rhino.Plugin.CPP</TemplateID>
        <ProjectType>VC</ProjectType>
        <PlatformTag>Windows</PlatformTag>
        <ProjectTypeTag>Desktop</ProjectTypeTag>
        <ProjectTypeTag>Rhino</ProjectTypeTag>
        <LanguageTag>cpp</LanguageTag>
        <Icon>icon.ico</Icon>
        <DefaultName>MyRhinoPlugin</DefaultName>

        <CreateNewFolder>true</CreateNewFolder>
        <ProvideDefaultName>true</ProvideDefaultName>
        <LocationField>Enabled</LocationField>
        <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    </TemplateData>
    <TemplateContent>
        <ProjectCollection/>
        <CustomParameters>
            <CustomParameter Name="$language$" Value="cpp" />
            <CustomParameter Name="$uistyle$" Value="none"/>
            <CustomParameter Name="$groupid$" Value="Rhino.Plugin" />
            <CustomParameter Name="SideWaffleNewProjNode" Value="Project\CPP\Rhino"/>
            <CustomParameter Name="SupportedParameters" Value="ExecutableLocation"/>
        </CustomParameters>
    </TemplateContent>
    <WizardExtension>
        <Assembly>Rhino.VisualStudio.Windows, Version=8.16.1.0, Culture=neutral, PublicKeyToken=null</Assembly>
        <FullClassName>Rhino.VisualStudio.Windows.Wizard.CppRhinoWizard</FullClassName>
    </WizardExtension>
    <WizardExtension>
        <Assembly>Microsoft.VisualStudio.TemplateEngine.Wizard, Version=1.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</Assembly>
        <FullClassName>Microsoft.VisualStudio.TemplateEngine.Wizard.TemplateEngineWizard</FullClassName>
    </WizardExtension>
</VSTemplate>